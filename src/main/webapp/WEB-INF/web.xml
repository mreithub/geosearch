<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>geoSearch</display-name>
  
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  
  
  <!-- Servlets -->
  <servlet>
    <servlet-name>SandBoxServiceImpl</servlet-name>
    <servlet-class>at.fakeroot.sepm.server.SandBoxServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>SandBoxServiceImpl</servlet-name>
    <url-pattern>/geosearch/sandboxarray</url-pattern>
  </servlet-mapping>
</web-app>
